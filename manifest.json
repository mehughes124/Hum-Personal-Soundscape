{
  "name": "Hum - Personal Soundscapes",
  "version": "0.0.2",
  "description": "Create a hum - your own personal soundscape. Fade out the world and focus in.",
  "permissions": ["storage", "background"],
  "background": {
      "scripts": [
      "scripts/howler.js",
      "background.js"
      ],
      "persistent": true
    },
  "browser_action": {
      "default_popup": "popup.html",
      "default_title": "Background Noise Generator",
      "default_icon": {
        "16": "images/hum_logo16.png",
        "32": "images/hum_logo32.png",
        "48": "images/hum_logo48.png",
        "128": "images/hum_logo128.png"
      }
    },
  "icons": {
      "16": "images/hum_logo16.png",
      "32": "images/hum_logo32.png",
      "48": "images/hum_logo48.png",
      "128": "images/hum_logo128.png"
    },
  "manifest_version": 2
}
